{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@static/*": [
          "./src/app/static/*"
      ],
      "@components/*": [
          "./src/app/components/*"
      ],
      "@/*": [
          "./src/*"
      ]
    },
    "target": "ES6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
